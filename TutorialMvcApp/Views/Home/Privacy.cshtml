@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>
<br>
<div><b>Persons Detail</b><br /></div>

<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>Adhar Id</th>
        <th>First Name </th>
        <th>Last Name </th>
        <th>Email</th>
        <th>Salary</th>
        <th>Company</th>
        <th>Department</th>
    </tr>
    @foreach (var d in Model)
    {
        <tr>
            <td>@d.AdharId</td>
            <td>@d.FirstName</td>
            <td>@d.LastName</td>
            <td>@d.Email</td>
            <td>@d.Salary</td>
            <td>@d.Company</td>
            <td>@d.Dept</td>
        </tr>
    }
</table>
